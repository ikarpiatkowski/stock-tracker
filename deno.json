{
  "tasks": {
    "tailwindcss:build": "deno run -A --watch npm:tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "dev": "deno task tailwindcss:build & deno run -A --watch --env-file ./src/server.tsx"
  },
  "imports": {
    "@/": "./src/",
    "@polygon.io/client-js": "npm:@polygon.io/client-js@^7.3.2",
    "@preact-icons/rx": "jsr:@preact-icons/rx@^1.0.12",
    "@radix-ui/react-icons": "npm:@radix-ui/react-icons@^1.3.0",
    "@radix-ui/react-popover": "npm:@radix-ui/react-popover@^1.1.2",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.1.0",
    "@std/path": "jsr:@std/path@^0.224.0",
    "chart.js": "npm:chart.js@^4.4.5",
    "class-variance-authority": "npm:class-variance-authority@^0.7.0",
    "clsx": "npm:clsx@^2.1.1",
    "date-fns": "npm:date-fns@^4.1.0",
    "lucide-react": "npm:lucide-react@^0.453.0",
    "postgres": "https://deno.land/x/postgres/mod.ts",
    "preact": "npm:preact@10.22.1",
    "preact/hooks": "npm:preact@10.22.1/hooks",
    "preact/jsx-runtime": "npm:preact@10.22.1/jsx-runtime",
    "preact-render-to-string": "https://esm.sh/preact-render-to-string@5.2.4",
    "react": "npm:react@^18.3.1",
    "react-day-picker": "npm:react-day-picker@^9.1.4",
    "tailwind-merge": "npm:tailwind-merge@^2.5.4",
    "tailwindcss": "npm:tailwindcss@^3.4.14",
    "tailwindcss-animate": "npm:tailwindcss-animate@^1.0.7",
    "xlsx": "https://esm.sh/xlsx"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "nodeModulesDir": "auto"
}
